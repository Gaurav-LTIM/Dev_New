<mvc:View
	height="100%"
	controllerName="sap.btp.projectxsuaaui.controller.Detail"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Page
		id="page"
		showHeader="true" >
				
		<customHeader>
			<Bar >
			    <contentLeft>
				     <Button id="back" text="back" enabled="true" press=".onNavBack" />
				</contentLeft>
				<contentRight>
					<Button id="edit" text="Edit" enabled="false" press="handleEditPress" />
					<Button id="save" text="Save" type="Emphasized" visible="false" press="handleSavePress" />
					<Button id="cancel" text="Cancel" visible="false" press="handleCancelPress" />
				</contentRight>
			</Bar>
		</customHeader>
	</Page>
</mvc:View>